<Window x:Class="heraldscraper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:heraldscraper"
        mc:Ignorable="d"
        Title="heraldScraper" Height="300" Width="400">
    <StackPanel Margin="10">
        <TextBlock Text="Herald Scraper" FontSize="32"/>
        <TextBlock Text="Scrapes full articles, even those which are paywalled."/>
        <TextBox x:Name="link"/>
        <Button x:Name="sumbit"
                Content="Submit"
                Click="Scrape"/>
    </StackPanel>
</Window>
